<h1>New article</h1>

<br>
<hr>
<br>

<%= link_to 'Return to articles', articles_path, class: 'btn btn-default' %>

<br>
<hr>
<br>

<%= simple_form_for(@article) do |f| %>
  <%= f.input :title %>
  <%= f.input :content %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit 'Create article', class: 'btn btn-success' %>
<% end %>
